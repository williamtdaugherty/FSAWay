

@{ Layout = null;}

<html ng-app="FsaWayApp">


<head>
  
    <base href="/" />
    <link rel="stylesheet" href="https://ajax.googleapis.com/ajax/libs/angular_material/0.9.4/angular-material.min.css">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:400,500,700,400italic">
    <meta name="viewport" content="initial-scale=1", user-scalable="no" />
    <link href="~/Content/angular-material.css" rel="stylesheet" />
   
    <style>
        md-toolbar h1 {
            margin: auto;
            font-weight: 400;
            color:aqua
            
        }

        md-toolbar.small h1 {
            font-size: 1.25em;
            margin: auto 0;
            padding-left: 16px;
        }

        #content {
            padding: 8px 40px;
        }

            #content.small {
                padding: 8px 16px;
            }
    </style>



</head>

<body ng-app="FsaWayApp"
      layout="column"
      ng-controller="LayoutController as lc">

    <md-toolbar>
        <h1>FSA Way</h1>
    </md-toolbar>
    <md-content flex>
        <h2>carousel/content placeholder</h2>
    </md-content>
    <md-button>
            <h5>
            <a href="/about">About Page</a> |<a href="/support">Support Page</a> | <a href="/signup">Sign Up</a> |<a href="/terms">Terms Page</a> |
            <a href="/products">Products Page</a> | <a href="fsacheck">FSACheck Page</a> | <a href="/persbalance">Personal Balance Page</a>
            </h5>
    </md-button>


    <ng-view></ng-view>


    <!-- Angular Material Dependencies -->
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.15/angular.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.15/angular-animate.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.15/angular-aria.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angular_material/0.9.4/angular-material.min.js"></script>
    <script src="~/Scripts/angular-resource.min.js"></script>
    <script src="~/Scripts/angular-route.min.js"></script>
    <script src="~/fsApps/app.js"></script>
    <script src="~/fsApps/controllers.js"></script>
    <script src="~/fsApps/AngularApp/Layout.js"></script>
    <script type="text/javascript">
        angular
            .module('FsaWayApp', ['ngMaterial', 'layout'])
            .config(function ($mdThemingProvider) {
                $mdThemingProvider.theme('default')
                .primaryPalette('light-blue')
                //.light();

            });
    </script>
</body>

</html>
